var superagent = require('superagent');
var agent = superagent.agent();
var theAccount = {
"username": "Tester",
"password": "tester1"
};

exports.login = function (request, done) {
console.log("might work login");
request
.post('/login')
.send(theAccount)
.end(function (err, res) {
if (err) {
console.log(err);
throw err;
}

agent.saveCookies(res);
done(agent);
});
};
console.log(theAccount);
console.log("might work login 4");